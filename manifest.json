{
  "manifest_version": 3,
  "name": "MailHunter",
  "version": "1.0",
  "description": "Find emails from social platforms.",
  "permissions": [
    "storage",
    "tabs",
    "scripting"
  ],
  "host_permissions": [
    "https://www.google.com/*"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  }
}